{
  "require": {
    "php": ">=7.0.0"
  },
  "autoload": {
    "files": [
      "./app/core/include/initialization/initializations.php",
      "./app/core/external-api/carbon/vendor/autoload.php"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "./app/core/main/",
      "./app/core/middleware/",
      "./app/controller/",
      "./app/model/",
      "./app/ajax-handler/",
      "./app/core/external-api/",
      "./app/core/cn-trait/"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
